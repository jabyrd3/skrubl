{
  "name": "skrub",
  "version": "0.0.1",
  "description": "skribbl.io clone ",
  "main": "server.js",
  "scripts": {
    "watch": "NODE_ENV=development && NODE_PATH=./client/build/js/ && node_modules/.bin/webpack-dev-server --mode=development --inline --hot",
    "build": "rm -rf ./client/build && mkdir ./client/build && NODE_PATH=./client/build/js/ && NODE_ENV=production node_modules/.bin/webpack --mode=production",
    "server": "export NODE_ENV=development && ./node_modules/.bin/nodemon --inspect server/index.js"
  },
  "keywords": [
    "pictionary",
    "game",
    "open-source",
    "websocket"
  ],
  "author": "jordan byrd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jabyrd3/skrubl/issues"
  },
  "homepage": "https://github.com/jabyrd3/skrubl#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "milligram": "^1.3.0",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
